{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "new-cap": 0,
    "no-eval": 0,
    "no-return-assign": 0,
    "no-underscore-dangle": 0,
    "semi": ["error", "never"],
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "always",
      "asyncArrow": "ignore"
    }],
    "jsx-quotes": ["error", "prefer-single"],
    "react/jsx-first-prop-new-line": 0
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "globals": {
    "i18n": false,
    "extension": false,
    "__DEV__": false,
    "__PACKAGE_SERVER__": false
  }
}
